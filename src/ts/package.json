{
  "name": "alchemy-logging",
  "version": "0.0.0-REPLACEME",
  "description": "Alchemy Logging implementation in NodeJS/TypeScript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "NODE_ENV=test nyc mocha --exit --require ts-node/register 'test/**/*.ts'",
    "lint": "./node_modules/tslint/bin/tslint --project . --config ./tslint.json",
    "clean": "rm -rf dist",
    "build": "tsc",
    "prepare": "npm run build",
    "repl": "ts-node"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:IBM/alchemy-logging.git"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "coverage/**",
      "dist/**",
      "test/**"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "all": true
  },
  "author": "Gabe Goodhart",
  "license": "MIT",
  "dependencies": {
    "@types/node": "^24.0.1",
    "deepcopy": "^2.0.0",
    "typescript": "^5.7.3"
  },
  "devDependencies": {
    "@types/chai": "^5.0.0",
    "@types/mocha": "^10.0.0",
    "chai": "^6.0.1",
    "deep-equal": "^2.0.5",
    "memory-streams": "^0.1.3",
    "mocha": "^11.1.0",
    "nyc": "^17.0.0",
    "rewire": "^8.0.0",
    "ts-node": "^10.2.0",
    "tslint": "^6.0.0-beta0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-no-unused-expression-chai": "^0.1.4"
  }
}
